      <describable-list-property>
        <originalValue class="hudson.util.DescribableList">
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description></description>
            <command># Remove screenshots from previous SWTbot tests so that only relevant ones are present after a build
rm -rf &quot;$WORKSPACE/plugins/org.eclipse.sirius.tests.swtbot/screenshots&quot;

# Make sure we have a properly configured window manager for SWTbot tests
/usr/bin/icewm &amp;
sleep 1

rm -f plugins/org.eclipse.sirius.tests*/org_eclipse_sirius_tests_*.txt
export SWT_GTK3=0</command>
          </hudson.tasks.Shell>
          <maven-builder>
            <disabled>false</disabled>
            <config>
              <installationId>apache-maven-latest</installationId>
              <goals>clean
integration-test
-B</goals>
              <privateRepository>true</privateRepository>
              <privateTmpdir>false</privateTmpdir>
              <pomFile>packaging/org.eclipse.sirius.parent/pom.xml</pomFile>
              <properties>
                <entries>
                  <entry name="platform-version-name" value="$PLATFORM"/>
                </entries>
              </properties>
              <errors>false</errors>
              <verbosity>NORMAL</verbosity>
              <offline>false</offline>
              <snapshotUpdateMode>NORMAL</snapshotUpdateMode>
              <profiles>
                <string>full</string>
                <string>headless</string>
                <string>incubation</string>
                <string>$SUITE</string>
              </profiles>
              <recursive>true</recursive>
              <checksumMode>NORMAL</checksumMode>
              <failMode>NORMAL</failMode>
              <makeMode>NONE</makeMode>
              <settingsId>NONE</settingsId>
              <globalSettingsId>NONE</globalSettingsId>
              <toolChainsId>NONE</toolChainsId>
            </config>
          </maven-builder>
        </originalValue>
        <propertyOverridden>false</propertyOverridden>
      </describable-list-property>
