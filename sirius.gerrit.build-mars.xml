                <string>pack</string>
              </profiles>
              <recursive>true</recursive>
              <checksumMode>NORMAL</checksumMode>
              <failMode>NORMAL</failMode>
              <makeMode>NONE</makeMode>
              <settingsId>NONE</settingsId>
              <globalSettingsId>NONE</globalSettingsId>
              <toolChainsId>NONE</toolChainsId>
            </config>
          </maven-builder>
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description></description>
            <command># Adjust target platform definition to consume the Sirius we just built locally instead of the latest published nightly
if [ &quot;$GERRIT_BRANCH&quot; = &quot;master&quot; ]; then
  STREAM=&quot;latest&quot;
else
  STREAM=$(echo &quot;$GERRIT_BRANCH&quot; | sed -e &apos;s/v//&apos;)
  # Remove &quot;-x&quot; of specific branch (with 3 points in its name)
  if [ $(grep -o &quot;\.&quot; &lt;&lt;&lt; &quot;$GERRIT_BRANCH&quot; | wc -l) = &quot;3&quot; ]; then
    STREAM=$(echo &quot;$STREAM&quot; | sed -e &apos;s/-x//&apos;)
  fi
fi
if [ -e &quot;releng/org.eclipse.sirius.targets/sirius_tests_$PLATFORM.target&quot; ]; then
    sed -i -e &quot;s!http://download.eclipse.org/sirius/updates/nightly/$STREAM/$PLATFORM!file://$WORKSPACE/packaging/org.eclipse.sirius.update/target/repository!&quot; &quot;releng/org.eclipse.sirius.targets/sirius_tests_$PLATFORM.target&quot;
fi
</command>
          </hudson.tasks.Shell>
          <maven-builder>
            <disabled>false</disabled>
            <config>
              <installationId>apache-maven-latest</installationId>
              <goals>clean package</goals>
              <privateRepository>true</privateRepository>
              <privateTmpdir>false</privateTmpdir>
              <pomFile>packaging/org.eclipse.sirius.tests.parent/pom.xml</pomFile>
              <properties>
                <entries>
                  <entry name="platform-version-name" value="$PLATFORM"/>
                </entries>
              </properties>
              <errors>false</errors>
              <verbosity>NORMAL</verbosity>
              <offline>false</offline>
              <snapshotUpdateMode>NORMAL</snapshotUpdateMode>
              <recursive>true</recursive>
              <checksumMode>NORMAL</checksumMode>
              <failMode>NORMAL</failMode>
              <makeMode>NONE</makeMode>
              <settingsId>NONE</settingsId>
              <globalSettingsId>NONE</globalSettingsId>
              <toolChainsId>NONE</toolChainsId>
            </config>
          </maven-builder>
        </originalValue>
        <propertyOverridden>false</propertyOverridden>
      </describable-list-property>
    </entry>
