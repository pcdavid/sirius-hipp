          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <description></description>
            <command>if [ -f ./releng/org.eclipse.sirius.releng/checks.sh ]; then
  sh ./releng/org.eclipse.sirius.releng/checks.sh
fi
</command>
          </hudson.tasks.Shell>
          <maven-builder>
            <disabled>false</disabled>
            <config>
              <installationId>apache-maven-latest</installationId>
              <goals>clean package</goals>
              <privateRepository>true</privateRepository>
