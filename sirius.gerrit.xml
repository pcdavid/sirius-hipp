    <entry>
      <string>builders</string>
      <describable-list-property>
        <originalValue class="hudson.util.DescribableList">
          <hudson.tasks.Shell>
            <command>if [ &quot;$PLATFORM&quot; = &quot;luna&quot; -o &quot;$SUITE&quot; = &quot;gerrit-junit&quot; ]; then

message=&quot;Listening for VNC connections on TCP port&quot;
logFileURL=&quot;https://hudson.eclipse.org/sirius/job/sirius.gerrit/$BUILD_NUMBER/PLATFORM=$PLATFORM,SUITE=$SUITE/consoleText&quot;
attempt=1
while ! ( curl -s &quot;$logFileURL&quot; | grep -q &quot; $message &quot; ); do
    sleep 5
    if [ &quot;$attempt&quot; -gt &quot;4&quot; ]; then
        echo &quot;Xvnc not ready; abandonning.&quot;
        exit 1
    else
        attempt=$(( $attempt + 1 ))
    fi
done

# Make sure we have a properly configured window manager for SWTbot tests
/usr/bin/metacity --display=$DISPLAY --replace --sm-disable &amp;
WM_PID=$!
sleep 1
/usr/bin/metacity-message disable-keybindings

# Build the core
/shared/common/apache-maven-latest/bin/mvn -V -B -DBUILD_SIGN=false -Dmaven.repo.local=&quot;$WORKSPACE/.maven/repo&quot; -DPLATFORM=&quot;$PLATFORM&quot; -Dplatform-version-name=&quot;$PLATFORM&quot; -f packaging/org.eclipse.sirius.parent/pom.xml clean package

# Adjust the target platform used to build/execute the tests to consume the local version of the core we just built
if [ &quot;$GERRIT_BRANCH&quot; = &quot;master&quot; ]; then
    export STREAM=&quot;latest&quot;
else
    export STREAM=$(echo $GERRIT_BRANCH | sed -e &apos;s/v//&apos;)
fi
sed -i -e &quot;s!http://download.eclipse.org/sirius/updates/nightly/$STREAM/$PLATFORM!file://$WORKSPACE/packaging/org.eclipse.sirius.update/target/repository!&quot; &quot;releng/org.eclipse.sirius.targets/sirius_tests_$PLATFORM.target&quot;

# Build the test, and if we are on master and build for the reference platform, also run the Gerrit-specific test-suites
if [ &quot;$GERRIT_BRANCH&quot; = &quot;master&quot; -a &quot;$PLATFORM&quot; = &quot;luna&quot; ]; then
    /shared/common/apache-maven-latest/bin/mvn -V -B -DBUILD_SIGN=false -Dmaven.repo.local=&quot;$WORKSPACE/.maven/repo&quot; -DPLATFORM=&quot;$PLATFORM&quot; -Dplatform-version-name=&quot;$PLATFORM&quot; -f packaging/org.eclipse.sirius.tests.parent/pom.xml -P$SUITE clean integration-test
else
    /shared/common/apache-maven-latest/bin/mvn -V -B -DBUILD_SIGN=false -Dmaven.repo.local=&quot;$WORKSPACE/.maven/repo&quot; -DPLATFORM=&quot;$PLATFORM&quot; -Dplatform-version-name=&quot;$PLATFORM&quot; -f packaging/org.eclipse.sirius.tests.parent/pom.xml clean package
fi

if ps --pid &quot;$WM_PID&quot; -o pid,args | grep -q metacity ; then
  kill &quot;$WM_PID&quot;
fi

fi
</command>
          </hudson.tasks.Shell>
        </originalValue>
