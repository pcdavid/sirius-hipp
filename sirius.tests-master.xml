      <describable-list-property>
        <originalValue class="hudson.util.DescribableList">
          <maven-builder>
            <config>
              <installationId>apache-maven-latest</installationId>
              <goals>clean
package
-B
</goals>
              <privateRepository>true</privateRepository>
              <privateTmpdir>false</privateTmpdir>
              <pomFile>packaging/org.eclipse.sirius.parent/pom.xml</pomFile>
              <properties>
                <entries>
                  <entry name="platform-version-name" value="$PLATFORM"/>
                </entries>
              </properties>
              <errors>false</errors>
              <verbosity>NORMAL</verbosity>
              <offline>false</offline>
              <snapshotUpdateMode>NORMAL</snapshotUpdateMode>
              <recursive>true</recursive>
              <checksumMode>NORMAL</checksumMode>
              <failMode>NORMAL</failMode>
              <makeMode>NONE</makeMode>
              <settingsId>NONE</settingsId>
              <globalSettingsId>NONE</globalSettingsId>
              <toolChainsId>NONE</toolChainsId>
            </config>
          </maven-builder>
          <hudson.tasks.Shell>
            <command># Adjust target platform definition to consume the Sirius we just built locally instead of the latest published nightly
sed -i -e &quot;s!http://download.eclipse.org/sirius/updates/nightly/latest/$PLATFORM!file://$WORKSPACE/packaging/org.eclipse.sirius.update/target/repository!&quot; &quot;releng/org.eclipse.sirius.targets/sirius_tests_$PLATFORM.target&quot;

# Remove screenshots from previous SWTbot tests so that only relevant ones are present after a build
rm -rf &quot;$WORKSPACEp/lugins/org.eclipse.sirius.tests.swtbot/screenshots&quot;

# Make sure we have a properly configured window manager for SWTbot tests
/usr/bin/metacity --display=$DISPLAY --replace --sm-disable &amp;
sleep 1
/usr/bin/metacity-message disable-keybindings

export SWT_GTK3=0</command>
          </hudson.tasks.Shell>
          <maven-builder>
            <config>
              <installationId>apache-maven-latest</installationId>
              <goals>clean
integration-test
-B
</goals>
              <privateRepository>true</privateRepository>
              <privateTmpdir>false</privateTmpdir>
              <pomFile>packaging/org.eclipse.sirius.tests.parent/pom.xml</pomFile>
              <properties>
                <entries>
