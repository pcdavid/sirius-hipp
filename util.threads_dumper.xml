<project>
  <actions/>
  <description>This job uses &lt;code&gt;jstack&lt;/code&gt; on every Java process owned by the Sirius HIPP user to dump its thread state in the console.&#xd;
&#xd;
This is usefull if you need some insights regarding a deadlock occuring during the CI for instance.</description>
  <project-properties class="concurrent-hash-map">
    <entry>
      <string>hudson-plugins-disk_usage-DiskUsageProperty</string>
      <base-property>
        <originalValue class="hudson.plugins.disk_usage.DiskUsageProperty"/>
        <propertyOverridden>false</propertyOverridden>
      </base-property>
    </entry>
    <entry>
      <string>builders</string>
      <describable-list-property>
        <originalValue class="hudson.util.DescribableList">
          <hudson.tasks.Shell>
            <disabled>false</disabled>
            <command>echo &apos;##################### Java Processes ##################################&apos;
ps -ef | grep java |grep sirius 
echo &apos;##################### Java processes states ###########################&apos;
for i in $(ps -ef | grep java |grep sirius| awk &apos;{print $2}&apos;); do echo &quot;#######################################&quot; ; /opt/public/common/jdk1.6.0_45/bin/jstack $i ; done;
</command>
          </hudson.tasks.Shell>
        </originalValue>
        <propertyOverridden>false</propertyOverridden>
      </describable-list-property>
    </entry>
